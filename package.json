{
  "name": "netsblocks",
  "version": "0.0.1",
  "description": "Visual programming environment with networking support",
  "main": "server.js",
  "scripts": {
    "start": "node bin/netsblocks.js",
    "dev": "node node_modules/nodemon/bin/nodemon src/server.js",
    "testmon": "node node_modules/nodemon/bin/nodemon node_modules/mocha/bin/mocha --recursive test/server",
    "test": "node node_modules/mocha/bin/mocha --recursive test/server",
    "test-client": "node test/client/integration.js",
    "test-cover": "node node_modules/istanbul/lib/cli.js --hook-run-in-context cover node_modules/mocha/bin/_mocha -- -R spec test/server/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NetsBlocks/server.git"
  },
  "author": "Brian Broll",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NetsBlocks/server/issues"
  },
  "homepage": "https://github.com/NetsBlocks/server",
  "dependencies": {
    "body-parser": "^1.12.4",
    "cookie-parser": "^1.3.5",
    "debug": "^2.1.3",
    "express": "^4.12.3",
    "express-session": "^1.11.3",
    "jade": "^1.9.2",
    "lodash": "^3.9.1",
    "mongodb": "^2.0.33",
    "ramda": "^0.13.0",
    "socket.io": "^1.3.5",
    "ws": "^0.7.1",
    "xml2js": "^0.4.10",
    "yargs": "^3.9.0"
  },
  "devDependencies": {
    "expect": "^1.6.0",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.1",
    "mocha-phantomjs": "^3.5.3",
    "nodemon": "^1.3.7",
    "sinon": "^1.16.1",
    "supertest": "^1.0.1"
  }
}
